{
  "name": "vue-rate-it",
  "version": "1.2.2",
  "description": "A highly customisable rating component for Vue 2.x.",
  "main": "src/star-rating.vue",
  "scripts": {
    "test": "gulp test",
    "watch-stars": "gulp watch -f=./src/reg/star-rating.js",
    "watch-hearts": "gulp watch -f=./src/reg/heart-rating.js",
    "watch-fa": "gulp watch -f=./src/reg/font-awesome-rating.js",
    "watch-music": "gulp watch -f=./src/reg/music-rating.js",
    "build-glyphs": "cd build && node font-awesome",
    "gulp": "gulp",
    "push-to-git": "git push",
    "push": "npm-run-all --sequential gulp push-to-git"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/craigh411/vue-rate-it.git"
  },
  "files": [
    "dist",
    "src"
  ],
  "keywords": [
    "star rating",
    "star",
    "vue",
    "vue.js",
    "vue 2"
  ],
  "author": "Craig Humphreys",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/craigh411/vue-rate-it/issues"
  },
  "homepage": "https://github.com/craigh411/vue-rate-it#readme",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.23.0",
    "babelify": "^7.3.0",
    "browserify": "^14.1.0",
    "browserify-shim": "^3.8.13",
    "font-awesome": "^4.7.0",
    "gulp": "^3.9.0",
    "gulp-flatten": "^0.3.1",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.8",
    "jasmine": "^2.5.3",
    "karma": "^1.4.0",
    "karma-browserify": "^5.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-spec-reporter": "0.0.26",
    "npm-run-all": "^4.0.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "vue": "^2.1.8",
    "vueify": "^9.4.0",
    "watchify": "^3.9.0",
    "yargs": "^7.0.1"
  },
  "browserify-shim": {
    "vue": "global:Vue"
  },
  "browserify": {
    "transform": [
      "babelify",
      "vueify",
      "browserify-shim"
    ]
  }
}
